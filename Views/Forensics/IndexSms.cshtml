        <th>
            @Html.DisplayNameFor(model => model.FDatetime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AttachmentId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FSheetNum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FSheetName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FNum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FCorrespond)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FDatetime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ForensicSMSID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ForensicSMSID }) |
