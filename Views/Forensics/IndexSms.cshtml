@model IEnumerable<IntelligenceCloud.Models.ForensicSMS>

@{
    ViewBag.Title = "IndexSms";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>IndexSms</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AttachmentId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FSheetNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FSheetName)
