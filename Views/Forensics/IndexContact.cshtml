        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FNote)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FSource)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isDeleted)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ForensicContactID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ForensicContactID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ForensicContactID })
        </td>
    </tr>
