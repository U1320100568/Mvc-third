@model IEnumerable<IntelligenceCloud.Models.ForensicContact>

@{
    ViewBag.Title = "IndexContact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>IndexContact</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
